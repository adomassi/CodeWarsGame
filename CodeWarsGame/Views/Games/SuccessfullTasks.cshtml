@model IEnumerable<string>

<p>
    @Html.ActionLink("Back to HiScores", "HiScores", "Games")
</p>
<table class="table">
    <tr>
        <th>
        </th>
        <th>
            @Html.DisplayName("Names of successfull tasks")
        </th>
        <th></th>
    </tr>
    @{ int rowNo = 0; }
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @(rowNo += 1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item)
            </td>
        </tr>}

</table>